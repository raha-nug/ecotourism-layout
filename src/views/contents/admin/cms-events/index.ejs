<div class="page-heading">
  <div class="page-title">
    <div class="row">
      <div class="col-12 col-md-6 order-md-1 order-last">
        <h3>Content Manage Events</h3>
        <p class="text-subtitle text-muted">
          A sortable, searchable, paginated table without dependencies thanks to
          simple-datatables
        </p>
      </div>
      <div class="col-12 col-md-6 order-md-2 order-first">
        <nav
          aria-label="breadcrumb"
          class="breadcrumb-header float-start float-lg-end"
        >
          <ol class="breadcrumb">
            <li class="breadcrumb-item">
              <a href="index.html">Manage Assesment</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">
              Manage Criteria
            </li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
  <section class="section">
    <div class="card">
      <div>
        <div class="card-header d-flex justify-content-between">
          <h4>List Events</h4>
          <a href="/admin/add-events">
            <button class="btn icon icon-left btn-primary">
              <i class="bi bi-plus"></i> Event
            </button>
          </a>
        </div>
      </div>
      <div class="card-body">
        <table class="table table-striped" id="table1">
          <thead>
            <tr>
              <th>No</th>
              <th>Title Event</th>
              <th>Date</th>
              <th>Location</th>

              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% event.forEach(function(event) { %>
            <tr>
              <td><%= event.no %></td>
              <td><%= event.title %></td>
              <td><%= event.date %></td>
              <td><%= event.location %></td>

              <td>
                <a href="/admin/edit-event" class="btn icon btn-warning"
                  ><i class="bi bi-pencil"></i
                ></a>
                <button
                  type="button"
                  id="deleteTheme"
                  class="btn icon btn-danger"
                >
                  <i class="bi bi-x"></i>
                </button>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </section>
</div>
