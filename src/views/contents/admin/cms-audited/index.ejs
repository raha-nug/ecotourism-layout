<div class="page-heading">
  <div class="page-title">
    <div class="row">
      <div class="col-12 col-md-6 order-md-1 order-last">
        <h3>Auditd Content Management</h3>
        <p class="text-subtitle text-muted">
          A sortable, searchable, paginated table without dependencies thanks to
          simple-datatables
        </p>
      </div>
      <div class="col-12 col-md-6 order-md-2 order-first">
        <nav
          aria-label="breadcrumb"
          class="breadcrumb-header float-start float-lg-end"
        >
          <ol class="breadcrumb">
            <li class="breadcrumb-item">
              <a href="index.html">Manage Landing Page</a>
            </li>
            <li class="breadcrumb-item active" aria-current="page">
              Manage Audited
            </li>
          </ol>
        </nav>
      </div>
    </div>
  </div>
  <section class="section">
    <div class="card">
      <div>
        <div class="card-header d-flex justify-content-between">
          <h4>Audited List</h4>
          <a href="/admin/add-audited">
            <button class="btn icon icon-left btn-primary">
              <i class="bi bi-plus"></i> Audited
            </button>
          </a>
        </div>
      </div>
      <div class="card-body">
        <table class="table table-striped" id="table1">
          <thead>
            <tr>
              <th>No</th>
              <th>News Headline</th>
              <th>Date</th>

              <th>Action</th>
            </tr>
          </thead>
          <tbody>
            <% news.forEach(function(news) { %>
            <tr>
              <td><%= news.no %></td>
              <td><%= news.title %></td>
              <td><%= news.date %></td>

              <td>
                <a href="/admin/edit-audited" class="btn icon btn-warning"
                  ><i class="bi bi-pencil"></i
                ></a>
                <button
                  type="button"
                  id="deleteTheme"
                  class="btn icon btn-danger"
                >
                  <i class="bi bi-x"></i>
                </button>
              </td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </section>
</div>
